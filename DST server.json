{
    "Don't Starve Together Dedicated Server": {
        "containers": {
            "dts": {
                "image": "jamesits/dst-server:latest",
                "launch_order": 1,
                "ports": {
                    "10999-11000": {
                        "description": "DST-standard server port",
                        "host_default": 10999-11000,
                        "label": "minecraft-bedrock-edition",
                        "protocol": "udp"
                        }
                     },
                     "12346-12347": {
                         "description": "DST-standard server port",
                         "host_default": 12346-12347,
                         "label": "minecraft-bedrock-edition",
                         "protocol": "udp"
                     },   
                "volumes": {
                    "/.klei/DoNotStarveTogether:/data": {
                        "description": "Choose a Share for the DST server data, including configuration, mods, etc.",
                        "label": "Data storage"
                        }
                    }
                }
            },
            "description": "DST server with selectable version",
            "ui": {
              "https": false,
              "slug": ""
              },
        "volume_add_support": true,
        "website": "https://github.com/Jamesits/docker-dst-server",
	      "version": "3.5"
      }

}
